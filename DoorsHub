local function YRIUTS_fake_script() -- undofb.LocalScript 
	local script = Instance.new('LocalScript', undofb)

	function undo()
		-- Get the Lighting service
		local lighting = game:GetService("Lighting")
	
		-- Reset the Ambient, OutdoorAmbient, and ShadowColor to their default values
		lighting.Ambient = Color3.fromRGB(127, 127, 127)
		lighting.OutdoorAmbient = Color3.fromRGB(63, 63, 63)
		lighting.ShadowColor = Color3.fromRGB(63, 63, 63)
	
		-- Reset the FogEnd and FogColor to their default values
		lighting.FogEnd = 100000
		lighting.FogColor = Color3.fromRGB(192, 192, 192)
	
		-- Re-enable shadows and global shadows
		lighting.GlobalShadows = true
		lighting.ShadowMap.Enabled = true
	end
	
	script.Parent.MouseButton1Click:Connect(undo)
	
end
coroutine.wrap(YRIUTS_fake_script)()
local function UJLU_fake_script() -- fullbrihgt.LocalScript 
	local script = Instance.new('LocalScript', fullbrihgt)

	function fb()
		-- Get the Lighting service
		local lighting = game:GetService("Lighting")
	
		-- Set the Ambient, OutdoorAmbient, and ShadowColor to white
		lighting.Ambient = Color3.fromRGB(255, 255, 255)
		lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
		lighting.ShadowColor = Color3.fromRGB(255, 255, 255)
	
		-- Set the FogEnd and FogColor to high values
		lighting.FogEnd = 100000
		lighting.FogColor = Color3.fromRGB(255, 255, 255)
	
		-- Disable shadows and global shadows
		lighting.GlobalShadows = false
		lighting.ShadowMap.Enabled = false
	end
	
	script.Parent.MouseButton1Click:Connect(fb)
	
end
coroutine.wrap(UJLU_fake_script)()
local function EVJQK_fake_script() -- otherstuff.LocalScript 
