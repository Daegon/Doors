local function AGPONCF_fake_script() -- KeyEsp.LocalScript 
	local script = Instance.new('LocalScript', KeyEsp)

	function keyesp()
		while true do
			-- Get the CurrentRooms folder from the workspace
			local currentRoomsFolder = workspace:FindFirstChild("CurrentRooms")
			if not currentRoomsFolder then
				warn("CurrentRooms folder not found in workspace")
				return
			end
	
			-- Find all KeyObtain models in the CurrentRooms folder
			local keyObtainModels = currentRoomsFolder:GetDescendants()
			for _, model in ipairs(keyObtainModels) do
				if model:IsA("Model") and model.Name == "KeyObtain" then
					-- Create a Highlight instance and parent it to the KeyObtain model
					local highlight = Instance.new("Highlight")
					highlight.Parent = model
				end
			end
	
			print("waiting for keys")
			wait(1)
		end
	end
	
	script.Parent.MouseButton1Click:Connect(keyesp)
	
end
coroutine.wrap(AGPONCF_fake_script)()
