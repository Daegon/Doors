local function RXEUMZ_fake_script() -- DoorEsp.LocalScript 
	local script = Instance.new('LocalScript', DoorEsp)

	function dooresp()
		while true do
			-- Get the CurrentRooms folder from the workspace
			local currentRoomsFolder = workspace:FindFirstChild("CurrentRooms")
			if not currentRoomsFolder then
				warn("CurrentRooms folder not found in workspace")
				return
			end
	
			-- Find all KeyObtain models in the CurrentRooms folder
			local keyObtainModels = currentRoomsFolder:GetDescendants()
			for _, model in ipairs(keyObtainModels) do
				if model:IsA("MeshPart") and model.Name == "Door" then
					-- Check if the model's parent is DoorFake or DoorNormal
					if model.Parent.Name ~= "DoorFake" and model.Parent.Name ~= "DoorNormal" then
						-- Create a Highlight instance and parent it to the KeyObtain model
						local highlight = Instance.new("Highlight")
						highlight.Parent = model
	
						-- Find the "Open" sound in the Door MeshPart's children
						local sound = model:FindFirstChild("Open")
						if sound then
							-- Connect a function to the sound's "Played" event
							sound.Played:Connect(function()
								-- Delete the highlight when the sound is played
								highlight:Destroy()
								-- Change the name of the MeshPart to prevent it from being highlighted again
								model.Name = "HighlightedDoor"
							end)
						end
					end
				end
			end
	
			print("waiting for real doors")
			wait(1)
		end
	end
	
	script.Parent.MouseButton1Click:Connect(dooresp)
	
end
coroutine.wrap(RXEUMZ_fake_script)()
